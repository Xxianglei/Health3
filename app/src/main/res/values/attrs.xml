<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ColumnView">
        <attr name="column_color" format="color" />
    </declare-styleable>

    <declare-styleable name="HistogramView">
        <attr name="column_per_screen" format="integer" />
        <attr name="histogram_color" format="color" />
        <attr name="date_text_color" format="color" />
        <attr name="date_text_size" format="dimension" />
    </declare-styleable>
    -
    <declare-styleable name="LineView">


        <attr name="lineWidth" format="dimension" />

        <attr name="dotRadius" format="dimension" />

        <attr name="dataFactor" format="integer" />

        <attr name="dashColor" format="color" />

        <attr name="isShowColumnLine" format="boolean" />

        <attr name="isShowRowLine" format="boolean" />

        <attr name="isLinearGradient" format="boolean" />

    </declare-styleable>
    -
    <declare-styleable name="CircleProgress">
        <!-- 进度条的背景色-->
        <attr name="bgColor" format="color" />
        <!-- 进度条的前景色 起始颜色-->
        <attr name="foreStartColor" format="color" />
        <!-- 进度条的前景色 结束颜色-->
        <attr name="foreEndColor" format="color" />
        <!-- 进度条的大小-->
        <attr name="progressWidth" format="dimension" />
        <!-- 最大进度-->
        <attr name="maxProgress" format="integer" />
        <!-- 现在的进度-->
        <attr name="currProgress" format="integer" />
        <!-- 进度条的起始位置-->
        <attr name="progressInitialPosition" format="integer" />
        <!-- 是否使用动画-->
        <attr name="useAnimation" format="boolean" />
        <!-- 使用渐变-->
        <attr name="useGradient" format="boolean" />
        <!-- 边角是否是圆的-->
        <attr name="isCircleCorner" format="boolean" />
        <!-- 是否是实心的-->
        <attr name="isSolid" format="boolean" />
    </declare-styleable>
    -
    <declare-styleable name="TickCircleProgress">
        <!-- 刻度条的粗细-->
        <attr name="tickWidth" format="dimension" />
        <!-- 刻度的颜色-->
        <attr name="tickColor" format="color" />
        <!-- 刻度条的高度-->
        <attr name="tickMarkHeight" format="dimension" />
    </declare-styleable>
    -
    <declare-styleable name="TextCircleProgress">
        <!-- 主标题颜色-->
        <attr name="headColor" format="color" />
        <!-- 副标题颜色-->
        <attr name="subHeadColor" format="color" />
        <!-- 主标题字体大小-->
        <attr name="headSize" format="dimension" />
        <!-- 副标题字体大小-->
        <attr name="subHeadSize" format="dimension" />
        <!-- 底部标题颜色-->
        <attr name="bottomHeadColor" format="color" />
        <!-- 底部标题大小-->
        <attr name="bottomHeadSize" format="dimension" />

    </declare-styleable>

    -
    <declare-styleable name="CurveChartView">

        <!--折线或者曲线 true为曲线，false为折线-->


        <attr name="curveOrBroken" format="boolean" />

        <attr name="max" format="integer" />

        <attr name="smartCircleColor" format="color" />

        <attr name="curveLineColor" format="color" />

        <attr name="smartFillColor" format="color" />

        <attr name="scaleColor" format="color" />

    </declare-styleable>


    -
    <declare-styleable name="BarChartView">

        <attr name="barWidth" format="integer" />

        <attr name="maxValue" format="integer" />

        <attr name="unit" format="string" />

        <attr name="horizentalLineNum" format="integer" />

        <attr name="lineColor" format="color" />

        <attr name="chartColor" format="color" />

        <attr name="textColor" format="color" />

        <attr name="gradientStartColor" format="color" />
        //渐变色起始颜色
        <attr name="gradientEndColor" format="color" />
        //渐变色结束颜色
    </declare-styleable>
    <declare-styleable name="MultiScrollNumber">

        <attr name="primary_number" format="integer" />

        <attr name="target_number" format="integer" />

        <attr name="number_size" format="integer" />

        <!--<attr name="number_color" format="color"/>-->


    </declare-styleable>
    -
    <declare-styleable name="LineChart">

        <attr name="StrokeWidth" format="dimension" />

        <attr name="FontSize" format="dimension" />

        <attr name="PointRadius" format="dimension" />

        <attr name="DarkColor" format="color" />

        <attr name="LightColor" format="color" />

        <attr name="DateTextColor" format="color" />

        <attr name="ShapeColor" format="color" />

    </declare-styleable>
    -
    <declare-styleable name="CirCleRefreshLayout">

        <attr name="AniBackColor" format="color" />

        <attr name="AniForeColor" format="color" />

        <attr name="CircleSmaller" format="integer" />

    </declare-styleable>

    -
    <declare-styleable name="DigitalView">

        <attr name="digiColor" format="color" />

        <attr name="digiTextSize" format="dimension" />

    </declare-styleable>


    -
    <declare-styleable name="DigitalGroupView">

        <attr name="digiGroupColor" format="color" />

        <attr name="digiGroupTextSize" format="dimension" />

        <attr name="digiGroupFigureCounts" format="integer" />

        <attr name="digiGroupInterval" format="dimension" />

    </declare-styleable>
    <declare-styleable name="LineIndicator">

        <attr name="lineAlertColor" format="color|reference" />

        <attr name="lineAlertSize" format="dimension" />

        <attr name="lineProgressBackground" format="color" />

        <attr name="lineProgressColor" format="color" />

        <attr name="lineIndicatorTextSize" format="dimension" />

        <attr name="lineIndicatorTextColor" format="color" />

        <attr name="lineIndicatorBackground" format="color" />
    </declare-styleable>
    -
    <declare-styleable name="DiscView">

        <!--圆圈线的宽度-->


        <attr name="dvStrokenWidth" format="dimension" />

        <!--圆圈颜色-->


        <attr name="dvStrokenColor" format="color" />

        <!--外装饰圆是否显示-->


        <attr name="dvOuterCircleShow" format="boolean" />

        <!--外装饰圆距离圆环边距-->


        <attr name="dvOuterCirclePad" format="dimension" />

        <!--内部装饰圆是否显示-->


        <attr name="dvInnerCircleShow" format="boolean" />

        <!--内部装饰圆距离圆环边距-->


        <attr name="dvInnerCirclePad" format="dimension" />

        <!--底部圆形是否显示-->


        <attr name="dvBottomCircleIsShow" format="boolean" />

        <!--圆形指示图片-->


        <attr name="dvIndicatorDraw" format="reference" />

        <!--圆盘开始旋转角度值-->


        <attr name="dvStartRotateAngle" format="integer" />

        <!--圆盘可旋转角度范围-->


        <attr name="dvAngleRotateSpan" format="integer" />

        <!--圆盘模式-->


        -
        <attr name="dvCircleMode">

            <enum name="normal" value="1" />

            <enum name="round" value="2" />

            <enum name="dot" value="3" />

        </attr>

    </declare-styleable>
    -<declare-styleable name="MiProgress">

    <attr name="miWidth" format="dimension|reference"/>

</declare-styleable>
    <declare-styleable name="MaterialViewPager">
        <attr name="viewpager_header" format="reference"/>
        <attr name="viewpager_logo" format="reference"/>
        <attr name="viewpager_pagerTitleStrip" format="reference"/>
        <attr name="viewpager_viewpager" format="reference"/>
        <attr name="viewpager_logoMarginTop" format="dimension"/>
        <attr name="viewpager_color" format="color"/>
        <attr name="viewpager_headerHeight" format="dimension"/>
        <attr name="viewpager_headerAlpha" format="float"/>
        <attr name="viewpager_imageHeaderDarkLayerAlpha" format="float"/>
        <attr name="viewpager_hideToolbarAndTitle" format="boolean"/>
        <attr name="viewpager_hideLogoWithFade" format="boolean"/>
        <attr name="viewpager_enableToolbarElevation" format="boolean"/>
        <attr name="viewpager_parallaxHeaderFactor" format="float"/>
        <attr name="viewpager_headerAdditionalHeight" format="dimension"/>
        <attr name="viewpager_displayToolbarWhenSwipe" format="boolean"/>
        <attr name="viewpager_transparentToolbar" format="boolean"/>
        <attr name="viewpager_animatedHeaderImage" format="boolean"/>
        <attr name="viewpager_disableToolbar" format="boolean"/>

    </declare-styleable>
</resources>